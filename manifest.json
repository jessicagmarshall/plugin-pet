{
  "name": "Plugin Pet",
  "description": "Encourages user to behave as if they had a real pet that needed their attention.",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": ["alarms", "notifications", "storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Pet Cat",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "cat_icon.jpeg"
  }
}
